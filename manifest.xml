<package>
    <description>"Motion Planning Libraries">A framework for robot motion planning. This library contains different state of the art motion planners.
    The current version supports optimization and sampling based planners. 
    Available motion planners are STOMP, TrajOpt and OMPL.</description>
    <author>Sankaranarayanan Natarajan/sankar.natarajan@dfki.de</author>
    <!-- Use the short version name from:https://spdx.org/licenses/ -->
    <license>BSD-3-Clause</license>
    <url>https://git.hb.dfki.de/dfki-planning/motion_planners.git</url>
    <logo>http://</logo>
    <depend package="base/cmake" />
    <depend package="base/types" />
    <depend package="base/logging" />
    <depend package="control/kdl_parser" />
    <depend package="control/urdfdom_headers" />
    <depend package="planning/kinematics_library" />
    <depend package="planning/collision_detection" />
    <depend package="planning/robot_model" />
    <depend package="slam/octomap" />
    <depend package="planning/ompl" />
    <depend package="boost" />
    <name>motion_planners</name>
    <maintainer>Sankaranarayanan Natarajan/sankar.natarajan@dfki.de</maintainer>
    <maintainer>Siddhant Kadwe/siddhant.kadwe@dfki.de</maintainer>
    <maintainer>Vamsi Origanti/vamsi.origanti@dfki.de</maintainer>
    <stage>-1</stage>
    <keywords>
        <keyword>planning</keyword>
    </keywords>
    <activity>active</activity>
    <use>single project</use>
</package>
